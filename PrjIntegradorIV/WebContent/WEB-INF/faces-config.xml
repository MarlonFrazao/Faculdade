<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/cadastraraplicacao.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastraraplicacao</from-outcome>
			<to-view-id>/cadastraraplicacao.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/cadastraraplicacao.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/ok.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/cadastrousuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastrousuario</from-outcome>
			<to-view-id>/cadastrousuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/cadastrousuario.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucesso</from-outcome>
			<to-view-id>/cadastrousuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/cadastrousuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>falha</from-outcome>
			<to-view-id>/cadastrousuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/homefaces.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/homefaces.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/homefaces.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>falha</from-outcome>
			<to-view-id>/ok.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>produtos</from-outcome>
			<to-view-id>/produtos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>carrinho</from-outcome>
			<to-view-id>/carrinho.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cadastroUsuario</from-outcome>
			<to-view-id>/cadastroUsuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cliente</from-outcome>
			<to-view-id>/clientes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
